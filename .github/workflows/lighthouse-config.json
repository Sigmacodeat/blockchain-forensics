{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "settings": {
        "onlyCategories": ["seo", "best-practices", "accessibility"]
      }
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "categories:seo": ["error", {"minScore": 0.9}],
        "categories:best-practices": ["warn", {"minScore": 0.85}],
        "categories:accessibility": ["warn", {"minScore": 0.9}]
      }
    }
  }
}
