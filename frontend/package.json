{
  "name": "blockchain-forensics-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "python3 scripts/ensure-en-i18n-minimal.py && python3 scripts/backfill-i18n-10pct.py && python3 scripts/validate-i18n-keys.py && python3 scripts/full-merge-i18n.py",
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build:analyze": "vite-bundle-visualizer && vite build",
    "build:optimized": "npm run i18n:audit && npm run seo:generate && npm run build",
    "export:static": "node scripts/export-static-routes.mjs",
    "postbuild": "npm run export:static && node scripts/generate-sitemaps.mjs && npm run hreflang:validate",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:all": "npm run test && npm run test:e2e && npm run bench",
    "bench": "vitest bench",
    "bench:watch": "vitest bench --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "i18n:check": "node scripts/find-missing-translations.mjs",
    "i18n:normalize": "node scripts/normalize-locales.mjs",
    "i18n:audit": "node scripts/audit-i18n.mjs",
    "i18n:audit:detailed": "node scripts/audit-locales.mjs",
    "i18n:report": "node scripts/find-missing-translations.mjs && cat i18n-missing-keys.json",
    "seo:generate": "node ../scripts/generate-sitemaps.mjs",
    "sitemaps:gen": "node scripts/generate-sitemaps.mjs",
    "hreflang:validate": "node scripts/hreflang-validate.mjs",
    "optimize": "bash ../optimize.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "fx:update": "node scripts/update-exchange-rates.mjs",
    "fx:update:dry": "node scripts/update-exchange-rates.mjs --dry"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/react": "^10.19.0",
    "@sentry/vite-plugin": "^4.4.0",
    "@tanstack/react-query": "^5.17.9",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.5",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cytoscape": "^3.33.1",
    "d3": "^7.8.5",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.18.0",
    "html2canvas": "^1.4.1",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.307.0",
    "monaco-editor": "^0.54.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-confetti-explosion": "^3.0.3",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^18.2.0",
    "react-force-graph-2d": "^1.25.4",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^16.0.1",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^6.21.1",
    "react-window": "^2.2.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.2.0",
    "vis-data": "^8.0.3",
    "vis-network": "^10.0.2",
    "web-vitals": "^5.1.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cytoscape": "^3.21.9",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.7.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-joyride": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.33",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.3.3",
    "vite": "^7.1.10",
    "vitest": "^2.1.4"
  }
}
